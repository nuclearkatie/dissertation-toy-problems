<simulation>

<control>
    <startmonth>1</startmonth>
    <startyear>2025</startyear>
    <dt>86400</dt>
    <duration>22</duration>
    <explicit_inventory>0</explicit_inventory>
    <explicit_inventory_compact>0</explicit_inventory_compact>
</control>

<archetypes>
    <spec><lib>cycamore</lib><name>Source</name></spec>
    <spec><lib>cycamore</lib><name>Storage</name></spec>
    <spec><lib>cycamore</lib><name>Sink</name></spec>
    <spec><lib>agents</lib><name>NullRegion</name></spec>
    <spec><lib>agents</lib><name>NullInst</name></spec>
</archetypes>

<facility>
    <name>Source</name>
    <config>
        <Source>
            <outcommod>commod</outcommod>
            <outrecipe>commod_recipe</outrecipe>
            <throughput>1</throughput>
        </Source>
    </config>
</facility>

<facility>
    <name>Storage</name>
    <config>
        <Storage>
            <in_commods><val>commod</val></in_commods>
            <out_commods><val>commod1</val></out_commods>
        </Storage>
    </config>
</facility>

<facility>
    <name>Storage1</name>
    <config>
        <Storage>
            <in_commods><val>commod1</val></in_commods>
            <out_commods><val>commod2</val></out_commods>
        </Storage>
    </config>
</facility>

<facility>
    <name>Sink</name>
    <config>
        <Sink>
            <in_commods>
                <val>commod2</val>
            </in_commods>
        </Sink>
    </config>
</facility>

<recipe>
    <name>commod_recipe</name>
    <basis>mass</basis>
    <nuclide><id>10010000</id><comp>1</comp></nuclide>
</recipe>

<region>
    <name>Region</name>
    <config><NullRegion/></config>
    <institution>
        <name>Inst</name>
        <config><NullInst/></config>
        <initialfacilitylist>
            <entry><prototype>Source</prototype><number>1</number></entry>
            <entry><prototype>Storage</prototype><number>1</number></entry>
            <entry><prototype>Storage1</prototype><number>1</number></entry>
            <entry><prototype>Sink</prototype><number>1</number></entry>
        </initialfacilitylist>
    </institution>
</region>

</simulation>